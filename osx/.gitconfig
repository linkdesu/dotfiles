[user]
	name = 谢奥林
	email = xieaolin@foxmail.com
[core]
    symlinks = false
    autocrlf = input
	excludesfile = /Users/xieaolin/.gitignore_global
    paper = less -r
    quotepath = false
[push]
    default = simple
[color]
    diff = auto
    status = auto
    branch = auto
    interactive = true
[branch]
      autosetuprebase = always
[alias]
    output = "!sh -c 'git archive -o ~/var/output.zip $1 $(git diff --name-only $2)' -"
    aa = add .
    ss = status
    cc = commit
    bb = branch
    lm = log --graph --pretty=format:'%h @%ci by %ce: %s %n' --after='1 month'
    ls = log --graph --pretty=format:'%h @%ci by %ce: %s %n'
[diff]
    tool = meld
[merge]
    tool = meld
[mergetool]
    keepBackup = false
[difftool "sourcetree"]
	cmd = " "
	path = 
[mergetool "sourcetree"]
	cmd = " "
	trustExitCode = true
[commit]
	template = /Users/xieaolin/.stCommitMsg
